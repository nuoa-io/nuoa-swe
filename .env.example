# NUOA SWE Environment Configuration
# Copy this file to .env and fill in your values
# DO NOT commit the .env file to git

# ==========================================
# Admin Console Configuration
# ==========================================

# Admin Base URL (choose based on environment)
ADMIN_BASE_URL=https://admin.beta.nuoa.io
# ADMIN_BASE_URL=https://admin.gamma.nuoa.io
# ADMIN_BASE_URL=https://admin.nuoa.io

# ==========================================
# Tenant Configuration
# ==========================================

# Your tenant name
TENANT_NAME=your-tenant-name

# ==========================================
# User Credentials
# ==========================================

# User email for authentication
USER_EMAIL=user@example.com

# User password
USER_PASSWORD=your-password

# ==========================================
# AWS Configuration (optional)
# ==========================================

# AWS Profile (if not using default)
# AWS_PROFILE=aws-beta

# AWS Region
# AWS_REGION=ap-southeast-1

# ==========================================
# Development Settings
# ==========================================

# Debug mode
# DEBUG=false

# Stage environment
# STAGE=beta
